{
  "extends": ["react-app"],
  "plugins": ["react-hooks", "simple-import-sort"],
  "rules": {
    "react-hooks/rules-of-hooks": "error",
    "simple-import-sort/sort": "error",
    "no-multiple-empty-lines": "error",
    "comma-dangle": ["error", "always-multiline"],
    "eol-last": ["error", "always"],
    "semi": ["error", "never"],
    "quotes": ["error", "single"]
  }
}
